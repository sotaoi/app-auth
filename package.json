  "devDependencies": {
    "@app/auth": "file:./",
    "@app/omni": "file:../app-omni",
    "@sotaoi/api": "file:../packages/sotaoi-api",
    "@sotaoi/omni": "file:../packages/sotaoi-omni",
    "@types/node": "^14"
  },
  "dependencies": {
    "@app/omni": "^0.9.1",
    "@sotaoi/api": "^0.9.1",
    "@sotaoi/auth": "git+https://github.com/sotaoi/auth#6.1.1",
    "@sotaoi/omni": "^0.9.1",
    "forever": "^4.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "4.1.5"
  }
}
